@* 

<MudImage Style="border-radius: 5px; width=100%; object-fit:scale-down;" Src="@Imagem" />
    

@code {
    [Parameter]
    public ArtistaResponse? Artista { get; set; }

    public string? Imagem { get; set; }

    protected override void OnInitialized()
    {
        Imagem = Artista!.Banner!.Contains("Banner") ? $"https://localhost:7170{Artista!.Banner}" : $"{Artista!.Banner}";
    }

}
 *@